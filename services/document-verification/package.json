{
  "name": "@persona-chain/document-verification",
  "version": "1.0.0",
  "description": "Enterprise Document Verification Engine supporting 3,500+ document types from 200+ countries with AI/ML validation",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ml": "node tests/ml-validation.test.js",
    "test:accuracy": "node tests/accuracy-validation.test.js",
    "test:performance": "node tests/performance.test.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "npm run build:models && npm run build:templates",
    "build:models": "node scripts/build-ml-models.js",
    "build:templates": "node scripts/build-document-templates.js",
    "train:models": "python scripts/train_models.py",
    "validate:accuracy": "node scripts/validate-accuracy.js",
    "benchmark": "node benchmarks/verification-performance.js",
    "extract:features": "node scripts/feature-extraction.js",
    "classify:documents": "node scripts/document-classification.js",
    "update:database": "node scripts/update-document-database.js",
    "sync:regulatory": "node scripts/sync-regulatory-changes.js"
  },
  "keywords": [
    "document-verification",
    "identity-documents",
    "machine-learning",
    "ocr",
    "computer-vision",
    "fraud-detection",
    "regulatory-compliance",
    "global-documents",
    "ai-validation",
    "document-authentication"
  ],
  "author": "PersonaChain Team",
  "license": "MIT",
