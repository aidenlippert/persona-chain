    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/sharp": "^0.32.0",
    "@types/validator": "^13.11.7",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "sinon": "^17.0.1",
    "nock": "^13.4.0",
    "testcontainers": "^10.4.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "services/**/*.js",
      "ml/**/*.js",
      "!src/test/**",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testTimeout": 60000
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/persona-chain/document-verification.git"
  },
  "bugs": {
    "url": "https://github.com/persona-chain/document-verification/issues"
  },
  "homepage": "https://github.com/persona-chain/document-verification#readme",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/persona-chain"
  },
  "security": {
    "policy": "SECURITY.md",
    "advisory-database": true,
    "audit-signatures": true
  },
  "ml": {
    "frameworks": ["tensorflow", "pytorch", "onnx", "huggingface"],
    "models": ["ocr", "classification", "fraud-detection", "feature-extraction"],
    "accuracy": {
      "target": "99.5%",
      "minimum": "98.0%"
    },
    "performance": {
      "inference_time": "<500ms",
      "batch_size": 32,
      "memory_limit": "2GB"
    }
  },
  "documents": {
    "supported_types": 3500,
    "supported_countries": 200,
    "categories": [
      "identity",
      "passport",
      "driving_license",
      "visa",
      "residence_permit",
      "national_id",
      "voter_id",
      "tax_id",
      "social_security",
      "military_id",
      "student_id",
      "professional_license",
      "birth_certificate",
      "marriage_certificate",
      "diploma",
      "transcript",
      "bank_statement",
      "utility_bill",
      "employment_letter",
      "insurance_card"
    ]
  }
