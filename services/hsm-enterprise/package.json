{
  "name": "@persona-chain/hsm-enterprise",
  "version": "1.0.0",
  "description": "FIPS 140-2 Level 3 Hardware Security Module Integration for Enterprise Identity Infrastructure",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm run test:fips && npm run test:compliance",
    "test:fips": "node tests/fips-compliance.test.js",
    "test:compliance": "node tests/compliance-validation.test.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "npm run compile:providers",
    "compile:providers": "node scripts/compile-hsm-providers.js",
    "benchmark": "node benchmarks/hsm-performance.js",
    "validate:keys": "node scripts/validate-key-integrity.js",
    "rotate:keys": "node scripts/automated-key-rotation.js",
    "backup:hsm": "node scripts/hsm-backup.js",
    "audit:hsm": "node scripts/hsm-audit.js"
  },
  "keywords": [
    "hsm",
    "hardware-security-module",
    "fips-140-2",
    "enterprise-security",
    "key-management",
    "cryptography",
    "compliance",
    "aws-cloudhsm",
    "azure-hsm",
    "thales-luna",
    "identity-security"
  ],
  "author": "PersonaChain Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
