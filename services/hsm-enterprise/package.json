  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "sinon": "^17.0.1",
    "nock": "^13.4.0",
    "testcontainers": "^10.4.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "services/**/*.js",
      "providers/**/*.js",
      "!src/test/**",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testTimeout": 30000
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/persona-chain/hsm-enterprise.git"
  },
  "bugs": {
    "url": "https://github.com/persona-chain/hsm-enterprise/issues"
  },
  "homepage": "https://github.com/persona-chain/hsm-enterprise#readme",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/persona-chain"
  },
  "security": {
    "policy": "SECURITY.md",
    "advisory-database": true,
    "audit-signatures": true
  },
  "fips": {
    "compliance": "140-2-level-3",
    "validation": "required",
    "algorithms": [
      "AES-256-GCM",
      "RSA-4096",
      "ECDSA-P384",
      "SHA-256",
      "SHA-384",
      "SHA-512",
      "HMAC-SHA256"
    ]
  }
}