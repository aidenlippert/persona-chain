# PersonaPass Startup Environment Configuration
# Copy to .env.local and customize for your environment

# API Configuration
VITE_API_BASE_URL=https://api.personapass.dev
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Authentication
VITE_WEBAUTHN_ORIGIN=https://wallet.personapass.dev
VITE_WEBAUTHN_RP_ID=personapass.dev
VITE_WEBAUTHN_RP_NAME=PersonaPass Identity Wallet

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-pro
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.3

# Development Mode
VITE_DEV_MODE=true
VITE_DEBUG_LOGS=true
VITE_PERFORMANCE_MONITORING=true

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_SECURE_STORAGE=true
VITE_BIOMETRIC_FALLBACK=true

# Feature Flags
VITE_FEATURE_ZK_PROOFS=true
VITE_FEATURE_ANDROID_CREDS=true
VITE_FEATURE_EUDI_COMPLIANCE=true
VITE_FEATURE_GEMINI_AI=true
VITE_FEATURE_VIPER_GENERATOR=true

# Performance
VITE_BUNDLE_ANALYZER=false
VITE_LIGHTHOUSE_CI=false
VITE_PERFORMANCE_BUDGET=true

# Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_ERROR_REPORTING=true

# Build Configuration
VITE_BUILD_TIMESTAMP=auto
VITE_BUILD_VERSION=1.0.0-rc1
VITE_BUILD_COMMIT=auto

# Startup Specific
STARTUP_MODE=development
STARTUP_VELOCITY=high
STARTUP_SCALE_TARGET=1000000
STARTUP_SECURITY_LEVEL=paranoid