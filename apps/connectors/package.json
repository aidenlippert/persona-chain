{
  "name": "@personapass/connectors",
  "version": "1.0.0",
  "description": "PersonaPass Credential Connector Service",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@digitalbazaar/ed25519-signature-2020": "^5.0.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^4.0.0",
    "@noble/curves": "^1.9.2",
    "axios": "^1.10.0",
    "circomlib": "^2.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "plaid": "^36.0.0",
    "poseidon-lite": "^0.3.0",
    "snarkjs": "^0.7.5",
    "uint8arrays": "^5.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.14",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.1.10",
    "playwright": "^1.54.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
