# PersonaPass API Configuration
# Copy this to .env and add your actual API keys

# RapidAPI Configuration
VITE_RAPIDAPI_KEY=your_rapidapi_key_here

# Identity Verification APIs
VITE_TRULIOO_API_KEY=your_trulioo_api_key
VITE_JUMIO_API_TOKEN=your_jumio_api_token
VITE_ONFIDO_API_TOKEN=your_onfido_api_token

# Financial APIs
VITE_PLAID_CLIENT_ID=your_plaid_client_id
VITE_PLAID_SECRET=your_plaid_secret
VITE_PLAID_ACCESS_TOKEN=your_plaid_access_token
VITE_YODLEE_CLIENT_ID=your_yodlee_client_id
VITE_YODLEE_SECRET=your_yodlee_secret
VITE_MX_CLIENT_ID=your_mx_client_id
VITE_MX_SECRET=your_mx_secret

# Communication APIs
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token
VITE_TWILIO_SERVICE_SID=your_twilio_service_sid
VITE_TELESIGN_CUSTOMER_ID=your_telesign_customer_id
VITE_TELESIGN_API_KEY=your_telesign_api_key

# Professional APIs
VITE_LINKEDIN_CLIENT_ID=your_linkedin_client_id
VITE_LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
VITE_LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token
VITE_GITHUB_ACCESS_TOKEN=your_github_access_token

# Credit & Financial APIs
VITE_EXPERIAN_BASE_URL=https://us-api.experian.com
VITE_EXPERIAN_ACCESS_TOKEN=your_experian_access_token
VITE_EXPERIAN_CLIENT_ID=your_experian_client_id

# Development & Testing
VITE_ENABLE_API_MOCKING=true
VITE_API_TIMEOUT=10000
VITE_ENABLE_RATE_LIMITING=true